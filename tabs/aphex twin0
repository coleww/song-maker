        function jsonp_error(request, error, get_strings)
        {
            if (error == 'timeout') {
                error = 'Request timed out';
                suggest = 'Try again later';
            } else if (error == 'parsererror') {
                error = 'Internal server error';
                suggest = 'Contact system administrator';
            } else {
                error = 'Unknown error';
                suggest = 'Contact system administrator';
            }
        
            if (is_page_reloading || get_strings) {
                return {error: error, suggest: suggest};
            } else {
                $('#error_layer .alertmess').html(error+'<br><span class="small gray3">'+suggest+'</span>');
                $('#error_layer').removeClass('dn');
                open_shtorki();
            }
        }
        
        $.ajaxSetup({
            type: "POST",
            dataType: "json",
            timeout: 300000,
            error: function (request, error, get_strings) {
                if (error == 'timeout') {
                    error = 'Request timed out';
                    suggest = 'Try again later';
                } else if (error == 'parsererror') {
                    error = 'Internal server error';
                    suggest = 'Contact system administrator';
                } else {
                    error = 'Unknown error';
                    suggest = 'Contact system administrator';
                }

                if (is_page_reloading || get_strings) {
                    return {error: error, suggest: suggest};
                } else {
                    $('#error_layer .alertmess').html(error+'<br><span class="small gray3">'+suggest+'</span>');
                    $('#error_layer').removeClass('dn');
                    open_shtorki();
                }
            }
        });

        $.i18n.init({
            lng: 'en',
            resGetPath: '/translate',
            sendType: 'GET',
            customLoad: function(lngs, namespaces, options, cb){
                $.ajax({
                    url: options.resGetPath,
                    cache: options.cache,
                    method: options.sendType,
                    success: function(data, status, xhr) {
                        cb(null, data);
                    },
                    error : function(xhr, status, error) {
                        cb('failed loading resource.json error: ' + error);
                    },
                    dataType: "json",
                    async : true
                });
            },
            fallbackLng: ['en']
        },function(){
            $(document).trigger('i18n-load');
        });
    </script>

    <script>
    var psTracker = new function()
    {
        var events = [];
        
        function prepareHash(service, period, event)
        {
            return service + '_' + period + '_' + event;
        }
        
        this.track = function(service, period, event)
        {
            var hash = prepareHash(service, period, event)
            
            for (var i in events)
            {
                if (events[i] == hash)
                {
                    return false;
                }
            }
            
            events.push(hash);
            
            $.post('/event', {
                service:    service,
                period:     period,
                event:      event
            });
        }
        
    }
    
        
</script>    <!-- Track hit for PS stats -->
